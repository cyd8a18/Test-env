<?xml version="1.0"?>
<robot name="cube_gripper">
  <link name="world"/>
  <joint name="txj" type="prismatic">
    <limit lower="-1.0" upper="1.0"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <parent link="world"/>
    <child link="ty"/>
  </joint>
  <link name="ty"/>
  <joint name="tyj" type="prismatic">
    <limit lower="-1.0" upper="1.0"/>
    <axis xyz="0 1 0"/>
    <parent link="ty"/>
    <child link="tz"/>
  </joint>
  <link name="tz"/>
  <joint name="tzj" type="prismatic">
    <limit lower="0" upper="2.0"/>
    <axis xyz="0 0 1"/>
    <parent link="tz"/>
    <child link="rx"/>
  </joint>
  <link name="rx"/>
    <joint name="rxj" type="continuous">
    <axis xyz="1 0 0"/>
    <parent link="rx"/>
    <child link="ry"/>
  </joint>
  <link name="ry"/>
  <joint name="ryj" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="ry"/>
    <child link="rz"/>
  </joint>
  <link name="rz"/>
  <joint name="rzj" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="rz"/>
    <child link="visual_link"/>
  </joint>
  <link name="visual_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.06 0.075 0.2" />
      </geometry>
      <material name="blue">
        <color rgba="0.1 0.8 0.1 0.9" />
      </material>
    </visual>
   </link>
  <joint name="ee" type="fixed">
    <origin xyz="0 0 0.1" rpy="0 0 0" />
    <parent link="visual_link"/>
    <child link="ee_link"/>
    <axis xyz="0 0 1"/>
  </joint>
  <link name="ee_link">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1" />
      </geometry>
       <material name="brown">
        <color rgba="0.5 0.4 0.1 0.9" />
      </material>
    </visual>
  </link>

</robot>
